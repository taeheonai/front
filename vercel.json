{
  "rewrites": [
    {
      "source": "/login",
      "destination": "/auth/login"
    },
    {
      "source": "/signup",
      "destination": "/auth/signup"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard"
    },
    {
      "source": "/chatbot",
      "destination": "/chatbot"
    },
    {
      "source": "/esrs",
      "destination": "/esrs"
    },
    {
      "source": "/gri",
      "destination": "/gri"
    },
    {
      "source": "/materiality",
      "destination": "/materiality"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Manifest-Debug",
          "value": "true"
        }
      ]
    }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
